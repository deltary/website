    "date-fns": "^2.16.1",
    "next": "^10.2.3",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.1",
    "react": "^16.14.0",
    "react-big-calendar": "^0.24.6",
    "react-dom": "^16.14.0",
    "react-icons": "^3.11.0",
    "sass": "^1.29.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.4.0"
  }
